[
  {
    "name": "C",
    "extension": ".c",
    "compile": "gcc $s.c -o $s",
    "run": "$s.exe"
  },
  {
    "name": "C++",
    "extension": ".cpp",
    "compile": "g++ $s.cpp -o $s",
    "run": "$s.exe"
  },
  {
    "name": "Python",
    "extension": ".py",
    "compile": "",
    "run": "python $s.py"
  },
  {
    "name": "Java",
    "extension": ".java",
    "compile": "javac $s.java",
    "run": "java $s"
  },
  {
    "name": "JavaScript",
    "extension": ".js",
    "compile": "",
    "run": "node $s.js"
  },
  {
    "name": "Rust",
    "extension": ".rs",
    "compile": "rustc $s.rs",
    "run": "$s.exe"
  },
  {
    "name": "C#",
    "extension": ".cs",
    "compile": "csc $s.cs",
    "run": "$s.exe"
  },
  {
    "name": "Kotlin",
    "extension": ".kt",
    "compile": "kotlinc $s.kt -include-runtime -d $s.jar",
    "run": "java -jar $s.jar"
  }
]
